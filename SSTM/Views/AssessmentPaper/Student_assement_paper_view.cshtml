@model List<get_student_result_data>
@{
    ViewBag.Title = "Student Assement Paper View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Styles.Render("~/Theme/datatables")
<!-- Main content -->
<section class="content" style="padding-top:10px;">
    <!-- Default box -->
    <div class="card col-md-12">
        <div class="card-header row">
            <div class="col-9">
                <h3 class="card-title">Assessment Completed View </h3>
            </div>
            <div class="col-3 row" style="float: right;margin-left: 7px;">

            </div>
        </div>
        <div class="card-body">
            <div class="row">
                <div class="col-8">
                    <iframe src="@ViewBag.filename" width="600" height="600"></iframe>
                </div>
                <div class="col-4">
                    <table class="table" id="tblstudentmark" style=" position: relative; background: lightblue;">
                        <thead>
                            <tr>
                                @*<th>Sr.No</th>*@
                                <th>Division</th>
                                <th>Marks</th>
                                <th>Result</th>

                            </tr>
                        </thead>
                        <tbody>
                            @if (Model != null && Model.Count() > 0)
                            {
                                foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            <input type="text" placeholder="Divistion" readonly name="division' + count + '" value="@item.Divisionname" id="division' + count + '" style="width: 54px;" />
                                        </td>
                                        <td>
                                            <input type="text" placeholder="mark" readonly onkeypress="return isnumber(this);" value="@item.DivisionMarks" id="mark' + count + '" style="width:100px;" />
                                        </td>
                                        <td>
                                            <input type="text" placeholder="result" readonly id="result' + count + '" value="@item.Result" style="width:100px;" />
                                        </td>
                                    </tr>
                                }
                            }
                        </tbody>
                    </table>
                </div>

            </div>
        </div>
    </div>
</section>
