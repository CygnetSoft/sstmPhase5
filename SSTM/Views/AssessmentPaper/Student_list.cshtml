
@{
    ViewBag.Title = "Student_list";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Styles.Render("~/Theme/datatables")
<!-- Main content -->
<section class="content" style="padding-top:10px;">
    <!-- Default box -->
    <div class="card col-md-12">

        <div class="card-body">

            <table id="tblassesment_student" class="table">
                <thead>
                    <tr>

                        <th>Course Id</th>
                        <th>Student name</th>
                        <th>Fin</th>
                        <th>CourseName</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
    <input type="hidden" value="@ViewBag.courseid" id="courseid" />
    <input type="hidden" value="@ViewBag.batchid" id="batchid" />
</section>

@section scripts{
    @Scripts.Render("~/Theme/datatablesjs")
    <script>
        var getAssessment_studentListUrl = '@Url.Action("GetStudentList", "AssessmentPaper")';
        window.onload = function () {
            GetAssessment_student_list();
        }
    </script>
    <script src="~/Scripts/AssessmentPaper/index.js"></script>
}




